{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/static/:path*",
      "destination": "/public/static/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ]
}
